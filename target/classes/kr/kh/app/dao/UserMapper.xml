<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.kh.app.dao.UserDAO">
    <select id="selectUser" resultType="kr.kh.app.model.vo.UserVO"> SELECT * FROM USER WHERE user_id
        = #{user_id} </select>
    <insert id="insertMember"> INSERT INTO USER(user_id, user_pw, user_email, user_)
        VALUES(#{user.me_id},#{user.me_pw},#{user.me_email},"이용중") </insert>

</mapper>